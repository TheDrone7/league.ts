{
  "name": "@shieldbow/web",
  "version": "0.0.1",
  "description": "A browser-safe, lightweight part of the shieldbow library.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/**.ts\" \"tests/**/**.ts\"",
    "lint": "eslint --fix --color \"src/**/**.ts\" \"tests/**/**.ts\"",
    "format:test": "prettier --check \"src/**/**.ts\" \"tests/**/**.ts\"",
    "lint:test": "eslint --color \"src/**/**.ts\" \"tests/**/**.ts\"",
    "pretest": "run-s format:test lint:test build",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheDrone7/shieldbow.git"
  },
  "keywords": [
    "lol",
    "shieldbow",
    "league",
    "leagueoflegends",
    "typescript",
    "ts",
    "javascript",
    "js",
    "web"
  ],
  "author": "TheDrone7 <h@thedrone7.dev> (https://thedrone7.dev)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/TheDrone7/shieldbow/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/TheDrone7/shieldbow#readme",
  "dependencies": {
    "@discordjs/collection": "^1.5.1",
    "html-escaper": "^3.0.3",
    "tslib": "^2.6.0"
  },
  "optionalDependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.36.3",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "jest-junit": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
