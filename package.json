{
  "name": "shieldbow",
  "version": "1.3.0",
  "description": "An all purpose League of Legends API Client",
  "main": "dist/index.js",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.23.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/parser": "^5.11.0",
    "@vuepress/plugin-docsearch": "^2.0.0-beta.45",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-jest": "^28.0.2",
    "typescript": "^4.5.5",
    "vuepress": "^2.0.0-beta.43"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/**.ts\" \"tests/**/**.ts\"",
    "lint": "eslint --fix --color \"src/**/**.ts\" \"tests/**/**.ts\"",
    "pretest": "run-s format lint build",
    "test": "jest --runInBand",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "extract": "api-extractor run",
    "document": "tsc docs/generator.ts --outDir docs/ && node docs/generator.js",
    "docs:gen": "run-s build extract document"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheDrone7/shieldbow.git"
  },
  "keywords": [
    "league",
    "leagueoflegends",
    "lol",
    "ts",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "author": "TheDrone7 <hsbhatia765@outlook.com> (https://thedrone7.repl.co)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/TheDrone7/shieldbow/issues"
  },
  "homepage": "https://thedrone7.github.io/shieldbow/",
  "dependencies": {
    "@discordjs/collection": "^0.5.0",
    "axios": "^0.25.0",
    "fs-extra": "^10.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1"
  }
}
